{
  "name": "angular2-rollup-investigations",
  "version": "1.0.0",
  "description": "Angular2 Rollup Investigations",
  "main": "./src/main.ts",
  "scripts": {
    "clean:aot": "rm -rf aot",
    "clean:webpack": "rm -rf dist/webpack",
    "clean:webpack-aot": "npm run clean:aot && rm -rf dist/webpack-aot",
    "clean:rollup": "rm -rf dist/rollup",
    "clean:rollup-aot": "npm run clean:aot && rm -rf dist/rollup-aot",
    "dev:webpack": "webpack-dev-server --progress --port 8080",
    "build:webpack": "npm run clean:webpack && webpack --progress",
    "build:webpack-aot": "clean:webpack-aot && \"node_modules/.bin/ngc\" -p tsconfig-aot.json && webpack --config webpack-aot.config.js --progress",
    "build:rollup": "npm run clean:rollup && rollup -c rollup.config.js",
    "build:rollup-aot": "npm run clean:rollup-aot && rm -rf dist/rollup-aot && \"node_modules/.bin/ngc\" -p tsconfig-aot.json && rollup -c rollup-aot.config.js"
  },
  "author": "Marc Andrews",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.1.1",
    "@angular/compiler": "~2.1.1",
    "@angular/compiler-cli": "^2.1.2",
    "@angular/core": "~2.1.1",
    "@angular/forms": "~2.1.1",
    "@angular/http": "~2.1.1",
    "@angular/platform-browser": "~2.1.1",
    "@angular/platform-browser-dynamic": "~2.1.1",
    "@angular/platform-server": "^2.1.2",
    "@angular/router": "~3.1.1",
    "@angular/upgrade": "~2.1.1",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "lite-server": "^2.2.2",
    "raw-loader": "^0.5.1",
    "rollup": "^0.36.3",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "style-loader": "^0.13.1",
    "typescript": "^2.0.3",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  }
}
